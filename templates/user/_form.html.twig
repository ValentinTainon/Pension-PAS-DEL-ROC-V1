{{ form_start(form, { 'attr' : { 'class': 'user-form-container' } }) }}
   <div class="row">
      {{ form_row(form.nom) }}
      {{ form_row(form.prenom) }}
   </div>
   {{ form_row(form.adresse) }}
   <div class="row">
      {{ form_row(form.codePostal) }}
      {{ form_row(form.ville) }}
   </div>
   <div class="row">
      {{ form_row(form.email) }}
      {{ form_row(form.telephone) }}
   </div>
   <button class="btn">{{ button_label|default('Enregistrer') }}</button>
{{ form_end(form) }}
